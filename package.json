{
  "name": "fluence-cli-monorepo",
  "private": true,
  "type": "module",
  "license": "AGPL-3.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --cache-dir=.turbo",
    "on-each-commit": "turbo run on-each-commit --cache-dir=.turbo",
    "format": "prettier --write \"**/*.{ts,js,tsx}\""
  },
  "devDependencies": {
    "prettier": "3.2.5",
    "turbo": "1.13.3"
  },
  "packageManager": "yarn@4.2.2",
  "engines": {
    "node": "18.20.3"
  },
  "prettier": {}
}
