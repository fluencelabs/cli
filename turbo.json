{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "pack-linux-x64": {
      "outputs": ["dist/**"],
      "inputs": ["bin", "src", "package.json", "tsconfig.json"]
    },
    "pack-darwin-x64": {
      "outputs": ["dist/**"],
      "inputs": ["bin", "src", "package.json", "tsconfig.json"]
    },
    "pack-darwin-arm64": {
      "outputs": ["dist/**"],
      "inputs": ["bin", "src", "package.json", "tsconfig.json"]
    },
    "pack-win32-x64": {
      "outputs": ["dist/**"],
      "inputs": ["bin", "src", "package.json", "tsconfig.json", "nsis"]
    }
  }
}
